{
  "version": 2,
  "builds": [
    { 
      "src": "*.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/$1"
    },
    {
      "src": "/.*",
      "dest": "/"
    }
  ],
  "regions": [
    "sfo1"
  ]
}
